package GFG;

public class Swapping {
	public static void main(String[] args) {
		String str = "GeeksForGeeks";
		char temp;
		int j=1;
		char[] array = str.toCharArray();
		for(int i=0;i<str.length()&& j<str.length();i=i+2) {
			temp = array[i];
			array[i] = array[j];
			array[j] = temp;
			j = j+2;
		}
		String s = new String(array);
		System.out.print(s);
		
	}
}
