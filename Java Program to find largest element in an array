package GFG;

public class Test {
	static int largest(int arr[],int n) {
		
		int max = arr[0];
		for(int i=0;i<n;i++) {
			if(arr[i]>max) {
				max = arr[i];
			}
			
		}
		return max;
		
	}
	public static void main(String[] args) {
		int[] arr = {10,20,4};
		int n = arr.length;
		System.out.println(largest(arr,n));
		
	}
}
