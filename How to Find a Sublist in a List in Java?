package GFG;
import java.util.ArrayList;
import java.util.List;
public class Sublist {
	public static void main(String[] args) {
		List<Integer> l = new ArrayList<Integer>();
		l.add(4);
		l.add(5);
		l.add(6);
		l.add(7);
		l.add(8);
		List<Integer> w = l.subList(2, 4);
		System.out.println(w.toString());
		
	}
}
