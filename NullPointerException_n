package GFG;

public class NullPointerException_n {
	public static void main(String[] args) {
		int[] arr1 = {1,2,3,4};
		int[] arr2 = {2,3,4,5};
		int[] arr3 = null;
		try {
			for(int i=0;i<arr1.length;i++) {
				arr3[i] = arr1[i]/arr2[i];
			}
		}
		catch(NullPointerException e) {
			System.out.println(e);
		}
	}
}
