package GFG;
import java.util.*;
public class LeftRotate_new {
	public static void main(String[] args) {
		List<Integer> list = new ArrayList<Integer>();
		list.add(10);
		list.add(20);
		list.add(30);
		list.add(40);
		list.add(50);
		list.add(60);
		list.add(70);
		int temp = list.get(0);
		int size = list.size();
		for(int i=0;i<list.size()-1;i++) {
			list.set(i, list.get(i+1));
		}
		list.set(size-1, temp);
		System.out.println(list);
	}
}
