package GFG;
import java.io.*;
import java.util.*;
public class ArithmeticException_n {
	public static void main(String[] args) {
	try {
		int[] arr1 = {2,3,4,5};
		int[] arr2 = {3,0,7,8};
		int[] arr3 = new int[arr1.length];
		for(int i=0;i<arr1.length;i++) {
			arr3[i] = arr1[i]/arr2[i];
		}
	}
	catch(ArithmeticException e) {
		e.printStackTrace();
	}
	}
}
