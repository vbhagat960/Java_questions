package GFG;

import java.util.Scanner;

public class Q21 {
	public static void main(String[] args) {
		int i=0;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the value of n: ");
		int n = sc.nextInt();
		while(i<n) {
			for(int j=0;j<=i ;j++) {
				System.out.print("*");
			}
			System.out.println();
			i++;
		}
			sc.close();
	}
}
