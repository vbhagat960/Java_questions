package GFG;

import java.util.Scanner;

public class Q7 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number: ");
		int no = sc.nextInt();
		if(no%2==0) {
			System.out.println("EVEN");
		}
		else {
			System.out.println("ODD");
		}
		sc.close();
	}
}
