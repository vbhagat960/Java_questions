package GFG;

public class Fibonacci_Series {
	public static void main(String[] args) {
		int term1 = 0;
		int term2 = 1;
		int term3;
		int n = 15;
		System.out.print("0 1");
		while(n-2>0) {
			term3 = term1 + term2;
			term1 = term2;
			term2 = term3;
			
			System.out.print(" "+term3);
			n--;
		}
		
	}
}
