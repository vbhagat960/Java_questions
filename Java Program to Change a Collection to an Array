package GFG;
import java.util.*;
public class Change_Collections_to_Array {
	public static void main(String[] args) {
		List<String> list = new ArrayList<String>();
		list.add("Geeks");
		list.add("for ");
		list.add("Geeks");
		list.add("is");
		list.add("the");
		list.add("best");
		String[] str = list.toArray(new String[0]);
		for(int i=0;i<str.length;i++) {
			System.out.print(str[i]+" ");
		}
	}
}
